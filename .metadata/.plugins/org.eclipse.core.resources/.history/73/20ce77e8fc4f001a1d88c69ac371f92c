package com.example.demo.model;

public class Student {
	private int id;
	private String name;
	private int mark;
	
	public Student(int id, String name, int mark) {
		this.id = id;
		this.name = name;
		this.mark = mark;
	}
	public void setId(int id) {
		this.id = id;
	}
	public int getId() {
		return id;
	}
	public void setName(String name) {
		this.name = name;
	}
	public String getName() {
		return name;
	}
	public void setMark(int mark) {
		this.mark = mark;
	}
	public int getMark() {
		return mark;
	}
}
